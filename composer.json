{
    "name": "vinceg/fhalicenses",
    "description": "Manage HUD.GOV FHA Appraiser Licenses",
    "keywords": ["php", "amc", "fha", "hud", "hud.gov"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Gabriel",
            "email": "vadimg88@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/cache": "^4.1",
        "symfony/css-selector": "^4.1",
        "symfony/dom-crawler": "^4.1",
        "theiconic/name-parser": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "phpstan/phpstan": "^0.10"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "FHALicenses\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
